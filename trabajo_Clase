        <canvas id="myCanvas" width="400" height="400" style="border:1px solid #000000;">
        </canvas>
        <script>
            var c = document.getElementById("myCanvas");
            var ctx = c.getContext("2d");
            ctx.moveTo(100,50);
            ctx.lineTo(15,300);
            ctx.stroke();

            
            ctx.beginPath();
            ctx.moveTo(300,100);
            ctx.lineTo(300,300);
            ctx.lineTo(140,300);
            ctx.stroke();
            ctx.fill();
            
	ctx.beginPath();
	ctx.arc(150, 75, 50, 0, 2 * Math.PI);
	ctx.stroke();
    ctx.beginPath();
ctx.arc(50, 75, 50, 0, 2 * Math.PI);
ctx.fillStyle = "pink";
ctx.fill();
            
        </script>
